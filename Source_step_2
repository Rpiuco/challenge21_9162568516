##Source_step_2 -> Realiza o download dos arquivos iniciais disponibilizados pela producao
wget --no-check-certificate 'https://docs.google.com/uc?export=download&id=1tZj692JzzTRtxeuIPuOf3KVucB2q33Yv' -O genome/grch38.chr22.fasta.gz
wget --load-cookies /tmp/cookies.txt "https://docs.google.com/uc?export=download&confirm=$(wget --quiet --save-cookies /tmp/cookies.txt --keep-session-cookies --no-check-certificate 'https://docs.google.com/uc?export=download&id=1eosZV6s_T950IBKcSmcP4P-CKsGD11r8' -O- | sed -rn 's/.*confirm=([0-9A-Za-z_]+).*/\1\n/p')&id=1eosZV6s_T950IBKcSmcP4P-CKsGD11r8" -O rawData/amostra-lbb_R1.fq.gz && rm -rf /tmp/cookies.txt
wget --load-cookies /tmp/cookies.txt "https://docs.google.com/uc?export=download&confirm=$(wget --quiet --save-cookies /tmp/cookies.txt --keep-session-cookies --no-check-certificate 'https://docs.google.com/uc?export=download&id=1hwIkyPvleAaha_Wmx_AXXExlvMYUBD0J' -O- | sed -rn 's/.*confirm=([0-9A-Za-z_]+).*/\1\n/p')&id=1hwIkyPvleAaha_Wmx_AXXExlvMYUBD0J" -O rawData/amostra-lbb_R2.fq.gz && rm -rf /tmp/cookies.txt
wget --no-check-certificate 'https://docs.google.com/uc?export=download&id=1yI-28pC8b7k4X5m_mF35_8WpESVJimb6' -O pequeno-gabarito.vcf